
   
    <!-- Section for navigation buttons -->
    <div id="game-title3">
        <div id="game-title1" aria-disabled="true" disabled>
        <a class="statcard-desc" id="web-title">Community Feed</a>
          <div class="dropdown2">
            <a class="btn btn-outline-info mr-sm-2 logout-button1" type="button" id="dropdownMenu2" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Logout</a>
          </div>
          <div class="dropdown2">
            <a href="/profile" class="btn btn-outline-info mr-sm-2 logout-button1" type="button" id="dropdownMenu2" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Profile</a>
          </div>
          <div class="dropdown2">
            <a href="/feed" class="btn btn-outline-info mr-sm-2 logout-button1" type="button" id="dropdownMenu2" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Feed</a>
          </div>
        </div>
    </div>

    <!-- Sections to display reviews -->
{{#if displayReviewData.length}}
  {{#each displayReviewData}}
    <div class="card" id="game-title">
      <img src="/images/{{user.profileImage}}" id="blank-profile3" />
      <a style="font-size: 25px;">{{user.username}}</a>
      <div class="card" id="game-title5">
        <h3 id="topic-display">{{topic}}<span style="float: right; margin-right: 10px;">{{createdAt}}</span></h3>
        <hr>
        <img src="/reviewImages/{{this.postImage}}" id="review-image"/>
        <hr>
        <form class="delete-review1" >
          <a type="submit" class="delete-button1">Write a Comment</a>
          <a href="/comments/{{this.id}}" type="button" class="delete-button2">View Comments</a>
        </form>
        <h3 id="description-display"><span style="font-style: italic; margin-right: 15px; color:rgb(6, 9, 109); font-weight: bold; ">{{user.username}}:</span>{{description}}</h3>
        <form class="form-group3">
          <input type="text" class="comment-section" placeholder="Write a Comment....."/>
          <input type="hidden" name="reviewId" value="{{this.id}}">
          <button class="btn btn-primary" type="submit" id="submit-button3">Post</button>
        </form>
      </div>
    </div>
  {{/each}}
  <a href="#top" class="go-top-button">Go Back to the Top</a>
{{else}}
  <div class="card" id="game-title">
    <a href="/profile">Start A Post!</a>
  </div>
{{/if}}



<script type="text/javascript" src="./js/feed.js"></script>